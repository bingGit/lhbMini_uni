.navbar-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 9999999;
  /* background-color: #3075c6; */
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.navbar-text {
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  font-weight: 600;
}
.navbar-icon {
  position: fixed;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-radius: 64rpx;
  border: 0.5rpx solid rgba(255, 255, 255, 0.3);
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.icon {
  height: 25rpx;
  width: 25rpx;
  padding: 2rpx 10rpx 10rpx;
  display: inline-block;
  overflow: hidden;
}
.floatL {
  height: 18rpx;
  border: 0.5rpx solid rgba(255, 255, 255, 0.3);
  margin-top: 6rpx;
}
.navbar-loading {
  background: #fff;
  text-align: center;
}

