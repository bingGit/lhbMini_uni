<view><block wx:if="{{!navBarHidden}}"><nav-bar vue-id="704e9d00-1" navbar-data="{{nvabarData}}" bind:__l="__l"></nav-bar></block><view class="container"><block wx:if="{{isLogin}}"><view class="topTitle"><image class="avatar" src="{{avatar}}" data-event-opts="{{[['tap',[['avatarClick',['$event']]]]]}}" bindtap="__e"></image><view class="name">{{name}}</view><view class="uid">{{uid}}</view><image class="btn" src="/static/pages/images/exitBtn.png" mode="aspectFit" data-event-opts="{{[['tap',[['exitClick',['$event']]]]]}}" bindtap="__e"></image></view></block><block wx:else><block wx:if="{{!isLogin}}"><view data-event-opts="{{[['tap',[['loginClick',['$event']]]]]}}" class="topTitle" bindtap="__e"><image class="avatar" src="{{avatar}}"></image><view class="unLogin">{{name}}</view><image class="btn" src="/static/pages/images/loginBtn.png" mode="aspectFit"></image></view></block></block><block wx:for="{{menu}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{item.product_type!=-9999}}"><view class="list" data-dic="{{item}}" data-event-opts="{{[['tap',[['listClick',['$event']]]]]}}" bindtap="__e"><view class="title">{{item.title}}</view><view class="info">{{item.info}}</view><block wx:if="{{item.product_type.length>0}}"><image style="width:28rpx;height:40rpx;margin-right:30rpx;" src="/static/pages/images/right.png" mode="aspectFit"></image></block></view></block><block wx:else><button class="list listflex" open-type="contact" send-message-title="{{item.message_title}}" send-message-img="{{item.img}}" show-message-card="true" send-message-path="{{item.path}}"><view>{{item.title}}</view><image style="width:22rpx;height:40rpx;margin-right:30rpx;" src="/static/pages/images/right.png" mode="aspectFit"></image></button></block></block></block></view></view>