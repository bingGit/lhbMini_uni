<view><block a:if="{{!navBarHidden}}"><nav-bar vue-id="70449cd6-1" navbar-data="{{nvabarData}}" onVueInit="__l"></nav-bar></block><view class="container"><block a:if="{{isLogin}}"><view class="topTitle"><image class="avatar" src="{{avatar}}" data-event-opts="{{[['tap',[['avatarClick',['$event']]]]]}}" onTap="__e"></image><view class="name">{{name}}</view><view class="uid">{{uid}}</view><image class="btn" src="/static/pages/images/exitBtn.png" mode="aspectFit" data-event-opts="{{[['tap',[['exitClick',['$event']]]]]}}" onTap="__e"></image></view></block><block a:else><block a:if="{{!isLogin}}"><view data-event-opts="{{[['tap',[['loginClick',['$event']]]]]}}" class="topTitle" onTap="__e"><image class="avatar" src="{{avatar}}"></image><view class="unLogin">{{name}}</view><image class="btn" src="/static/pages/images/loginBtn.png" mode="aspectFit"></image></view></block></block><block a:for="{{menu}}" a:for-item="item" a:for-index="index" a:key="index"><block><block a:if="{{item.product_type!=-9999}}"><view class="list" data-dic="{{item}}" data-event-opts="{{[['tap',[['listClick',['$event']]]]]}}" onTap="__e"><view class="title">{{item.title}}</view><view class="info">{{item.info}}</view><block a:if="{{item.product_type.length>0}}"><image style="width:28rpx;height:40rpx;margin-right:30rpx;" src="/static/pages/images/right.png" mode="aspectFit"></image></block></view></block><block a:else><button class="list listflex" open-type="contact" send-message-title="{{item.message_title}}" send-message-img="{{item.img}}" show-message-card="true" send-message-path="{{item.path}}"><view>{{item.title}}</view><image style="width:22rpx;height:40rpx;margin-right:30rpx;" src="/static/pages/images/right.png" mode="aspectFit"></image></button></block></block></block></view></view>