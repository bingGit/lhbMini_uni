<view><block a:if="{{!navBarHidden}}"><nav-bar vue-id="0453c2e4-1" navbar-data="{{nvabarData}}" onVueInit="__l"></nav-bar></block><view class="container"><block a:for="{{albums}}" a:for-item="album" a:for-index="idx" a:key="idx"><view><block data-type="template" data-is="ablum-list" data-attr="album:album,idx:idx,selectId:selectId,muted:muted,coverHidden:coverHidden"><view class="ablum-list-container" data-url="{{album['url']}}" data-idx="{{idx}}" data-id="{{album['id']}}" data-event-opts="{{[['tap',[['goAlbum',['$event']]]]]}}" catchTap="__e"><block a:if="{{album['product_type']!=7010}}"><view class="ablum-list-item"><image class="ablum-list-img" src="{{album['cover']}}"></image></view></block><block a:if="{{album['product_type']==7010}}"><view class="ablum-view-c"><block a:if="{{coverHidden}}"><block><video class="ablum-video" autoplay="false" custom-cache="false" objectFit="fill" enable-play-gesture="true" id="myVideo" src="{{coverHidden?album['url']:''}}" controls></video></block></block><block a:else><block><cover-image class="ablum-list-img ablum-video-img" data-idx="{{idx}}" data-url="{{album['url']}}" data-id="{{album['id']}}" hidden="{{coverHidden}}" src="{{album['cover']}}" data-event-opts="{{[['tap',[['videoPlay',['$event']]]]]}}" onTap="__e"></cover-image></block></block></view></block></view></block></view></block><view class="{{('play-float'+(playFloating?' xuanzhun':''))}}" hidden="{{playFloatHidden}}" data-event-opts="{{[['tap',[['goDetail',['$event']]]]]}}" catchTap="__e"><image src="{{playFloatCover}}"></image></view></view></view>